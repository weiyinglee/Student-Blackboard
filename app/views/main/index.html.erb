<!-- Navbar rendering -->
<%= render "navbar" %>

<!-- CONTENT -->
<div class="body">
  <div class="row">
  	<!-- Schedule Column -->
  	<div class="col-md-4">
  		<div class="page-header">
  			<h1 class="page-title">Schedule</h1>

  		</div>
  		<div class="column-content">
  			 <ul>
  			 	<!-- Loop all the schedule in database -->
  			    <% @schedules.each do |schedule| %>
  			    	<!-- use if statement to filter out the logged in user's schedule but not other ppl's -->
  			    	<% if schedule.user == @user.account %>
			  	    	<li><span class="class-name"><%= schedule.classname %></span></li>
			  	    <% end %>
			    <% end %>
			</ul>
  		</div>
  	</div>
  	<!-- Note Column -->
  	<div class="col-md-4">
  		<div class="page-header">
  			<h1 class="page-title">Personal Note</h1>
  		</div>
  		<div class="column-content">
  			...
  		</div>
  	</div>
  	<!-- ??? Column -->
  	<div class="col-md-4"></div>
  </div>
</div>

<!-- Footer rendering -->
<%= render "footer" %>

